@model ComplainantViewModel
@{
    ViewBag.Title = "Edit Complaient";
    var projectName = Startup.StaticConfig["SolutionProjectName"];
    var photoPath = "~/img/no-image.jpg";
    var fileImage = "~/img/NoFile.png";
    var singleFileSaveToFolderViewModel = "";
    var singleFileSaveToDatabaseViewModel = "";
    var excelFileDataSaveToDatabaseViewModel = "";
    if (Model.SingleFileSaveToFolderViewModel.Name != null)
    {
        string[] extensation = Model.SingleFileSaveToFolderViewModel.Name.Split(".");
        if (extensation[1].ToLower() == "pdf")
        {
            singleFileSaveToFolderViewModel = "~/img/Pdf.png";
        }
        else if (extensation[1].ToLower() == "xlsx")
        {
            singleFileSaveToFolderViewModel = "~/img/Excel.png";
        }
        else if (extensation[1].ToLower() == "docx")
        {
            singleFileSaveToFolderViewModel = "~/img/Word.png";
        }
    }
    if (Model.SingleFileSaveToDatabaseViewModel.Name != null)
    {
        string[] extensation = Model.SingleFileSaveToDatabaseViewModel.Name.Split(".");
        if (extensation[1].ToLower() == "pdf")
        {
            singleFileSaveToDatabaseViewModel = "~/img/Pdf.png";
        }
        else if (extensation[1].ToLower() == "xlsx")
        {
            singleFileSaveToDatabaseViewModel = "~/img/Excel.png";
        }
        else if (extensation[1].ToLower() == "docx")
        {
            singleFileSaveToDatabaseViewModel = "~/img/Word.png";
        }
    }
    if (Model.ExcelFileDataSaveToDatabaseViewModel.Name != null)
    {
        string[] extensation = Model.ExcelFileDataSaveToDatabaseViewModel.Name.Split(".");
        if (extensation[1].ToLower() == "pdf")
        {
            excelFileDataSaveToDatabaseViewModel = "~/img/Pdf.png";
        }
        else if (extensation[1].ToLower() == "xlsx")
        {
            excelFileDataSaveToDatabaseViewModel = "~/img/Excel.png";
        }
        else if (extensation[1].ToLower() == "docx")
        {
            excelFileDataSaveToDatabaseViewModel = "~/img/Word.png";
        }
    }
}


